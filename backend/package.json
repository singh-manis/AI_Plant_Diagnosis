{
  "name": "ai-plant-backend",
  "version": "1.0.0",
  "description": "Backend for AI-Powered Urban Gardening Platform",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "prod": "NODE_ENV=production node server.js",
    "test:ai": "node test-ai-service.js",
    "test:ai:advanced": "node test-ai-service-advanced.js",
    "test:ai:intelligent": "node test-intelligent-ai.js",
    "pm2:start": "pm2 start ecosystem.config.js --env production",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:delete": "pm2 delete ecosystem.config.js",
    "pm2:logs": "pm2 logs",
    "pm2:monit": "pm2 monit",
    "test": "echo \"Error: no test specified\" && exit 1"
